{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "vahidrajabloo/cta",
    "version": "1.0.0",
    "title": "Call to Action",
    "category": "vahidrajabloo",
    "icon": "megaphone",
    "description": "A call-to-action section with title, description, and button.",
    "keywords": [
        "cta",
        "call to action",
        "banner",
        "promo"
    ],
    "textdomain": "vahidrajabloo-theme",
    "attributes": {
        "title": {
            "type": "string",
            "default": "Ready to get started?"
        },
        "description": {
            "type": "string",
            "default": "Join thousands of users who trust our platform."
        },
        "buttonText": {
            "type": "string",
            "default": "Get Started"
        },
        "buttonUrl": {
            "type": "string",
            "default": "#"
        },
        "variant": {
            "type": "string",
            "default": "default"
        }
    },
    "supports": {
        "html": false,
        "align": [
            "wide",
            "full"
        ],
        "color": {
            "background": true,
            "text": true
        }
    },
    "editorScript": "file:./index.js",
    "editorStyle": "file:./editor.css",
    "style": "file:./style.css",
    "render": "file:./render.php"
}